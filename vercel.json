{
    "rewrites": [
        {
            "source": "/((?!assets/|images/|favicon.ico|placeholder.svg|robots.txt).*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/images/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ],
    "outputDirectory": "dist"
}